#include<stdio.h>
void main()
{
    int lim, a=0,b=1,c;
    printf("enter the limit:");
    scanf("%d %d",&lim);
    printf("%d ",a);
    for (int i = 0; i < lim; i++)
    {
        a=b;
        b=c;
        c=a+b;
        printf("%d ",c);
    }
    
}

