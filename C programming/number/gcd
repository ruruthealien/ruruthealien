#include<stdio.h>
void main()
{
    int n1, n2, gcd;
    printf("Enter the number:");
    scanf("%d %d",&n1,&n2);
  for (int i = 1; i <=n1 && i<=n2 ; i++)
  {
    if(n1%i==0 && n2%i==0)
    {
        gcd = i;
    }
  }
    printf("GCD of %d and %d is: %d",n1,n2,gcd);
}


//Enter the number:30 45
//LCM of 30 and 45 is: 15
