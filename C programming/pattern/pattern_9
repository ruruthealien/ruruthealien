#include<stdio.h>
void main()
{
    int n;
    printf("Enter the number:");
    scanf("%d",&n);
   //first half of pattern
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j <= n; j++)
        {
            if(i==1 || i==n || j==1 || j==n)
            {
                printf("* ");
            }
            else
            {
                 printf("  ");
            }
        }
        printf("\n");
    }
}


//Enter the number:5
* * * * *
*       *
*       *
*       *
* * * * *
