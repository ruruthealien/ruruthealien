#include<stdio.h>
void main()
{
    int n;
    printf("Enter the number:");
    scanf("%d",&n);
    for (int i = 1; i <= n; i++)
    {
        for (int j = n; j >= i; j--)
        {
            printf("%d ",j);
        }
        printf("\n");
    }
}


//Enter the number:5
//5 4 3 2 1 
//5 4 3 2 
//5 4 3 
//5 4 
//5 
